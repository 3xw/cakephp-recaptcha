{
    "name": "3xw/cakephp-recaptcha",
    "description": "3xw/cakephp-recaptcha plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5",
        "google/recaptcha": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Trois\\Recaptcha\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Trois\\Recaptcha\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
